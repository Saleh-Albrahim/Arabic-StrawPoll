<!-- About -->
<div id="about" class="basic-4">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h2>عن البرنامج</h2>
                <p class="p-heading p-large">تم إنشاء هذا البرنامج من قبل</p>
            </div> <!-- end of col -->
        </div> <!-- end of row -->
        <div class="row">
            <div class="col-lg-12">

                <!-- Team Member -->
                <div class="team-member">
                    <div class="image-wrapper">
                        <img class="img-fluid" src="images/team-member-2.svg" alt="alternative">
                    </div> <!-- end of image wrapper -->
                    <p class="p-large"><strong>صالح البراهيم</strong></p>
                    <p class="job-title">Software Developer</p>
                    <span class="social-icons">
                        <span class="fa-stack">
                            <a href="https://github.com/Saleh-Albrahim">
                                <i class="fas fa-circle fa-stack-2x github"></i>
                                <i class="fab fa-github fa-stack-1x"></i>
                            </a>
                        </span>
                        <span class="fa-stack">
                            <a href="https://twitter.com/_SalehAlbrahim">
                                <i class="fas fa-circle fa-stack-2x twitter"></i>
                                <i class="fab fa-twitter fa-stack-1x"></i>
                            </a>
                        </span>
                        <span class="fa-stack">
                            <a href="https://www.linkedin.com/in/saleh--alibrahim">
                                <i class="fas fa-circle fa-stack-2x linkedin"></i>
                                <i class="fab fa-linkedin fa-stack-1x"></i>
                            </a>
                        </span>
                    </span> <!-- end of social-icons -->
                </div> <!-- end of team-member -->
                <!-- end of team member -->


                <!-- Contact -->
                <div id="contact" class="form-2">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-12">
                                <h2>معلومات التواصل</h2>
                                <ul class="list-unstyled li-space-lg">
                                    <li class="address">لا تتردد في الاتصال بنا</li>
                                </ul>
                            </div> <!-- end of col -->
                        </div> <!-- end of row -->
                        <div class="row text-center">
                            <div class="col">
                                <!-- Contact Form -->
                                <form id="contactForm" data-toggle="validator" data-focus="false">
                                    <div class="form-group">
                                        <input type="text" class="form-control-input" placeholder="العنوان" id="cname"
                                            required>
                                    </div>
                                    <div class="form-group">
                                        <input type="email" class="form-control-input" id="cemail" placeholder="الأيميل"
                                            required>
                                    </div>
                                    <div class="form-group">
                                        <textarea class="form-control-textarea" id="cmessage" placeholder="الرسالة"
                                            required></textarea>
                                    </div>
                                    <div class="form-group">
                                        <button type="submit" class="form-control-submit-button">إرسال الرسالة</button>
                                    </div>
                                    <div class="form-message">
                                        <div id="cmsgSubmit" class="h3 text-center hidden"></div>
                                    </div>
                                </form>
                                <!-- end of contact form -->

                            </div> <!-- end of col -->
                        </div> <!-- end of row -->
                    </div> <!-- end of container -->
                </div> <!-- end of form-2 -->
                <!-- end of contact -->
